{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog Post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "richtext"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "blog_post"
    }
  }
}
